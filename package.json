  "private": true,
  "dependencies": {
    "@material-tailwind/react": "^2.1.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "eslint": "^8.56.0",
    "firebase": "^10.4.0",
    "formik": "^2.4.5",
    "js-cookie": "^3.0.5",
    "react": "^18.2.0",
    "react-cookie": "^6.1.1",
    "react-cookie-consent": "^8.0.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "react-scripts": "^5.0.1",
    "react-spinners": "^0.13.8",
    "web-vitals": "^2.1.4",
    "yup": "^1.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:desarrollo": "react-scripts build && mv build tempBuild && mkdir build && mv tempBuild build/desarrollo",
    "build:qa": "react-scripts build && mv build tempBuild && mkdir build && mv tempBuild build/qa",
    "build:production": "react-scripts build && mv build tempBuild && mkdir build && mv tempBuild build/production",
    "test": "mocha \"src/Mocha_tests/**/*.mjs\"",
    "test-file": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "eqeqeq": "off",
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "babel-register": "^6.26.0",
    "chai": "^5.1.0",
    "identity-obj-proxy": "^3.0.0",
    "mocha": "^10.4.0",
    "sinon": "^17.0.1",
    "tailwindcss": "^3.3.3"
  }
